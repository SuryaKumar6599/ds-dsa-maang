    "from collections import defaultdict\n",
    "\n",
    "class FrequencyCounter:\n",
    "    def __init__(self):\n",
    "        self.freq = defaultdict(int)\n",
    "\n",
    "    def add(self, x):\n",
    "        self.freq[x] += 1\n",
    "        print(dict(self.freq))"
   ]
